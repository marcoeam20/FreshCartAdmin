<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="buttonloginstyle">
        <item name="android:background">@drawable/buttonloginstyle</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">15sp</item>

    </style>

    <style name="buttonregisterstyle">
        <item name="android:background">@drawable/buttonregisterstyle</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">15sp</item>

    </style>

    <style name="button_style">
        <item name="android:background">@drawable/button_style</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">15sp</item>

    </style>

    <style name="button_next">
        <item name="android:background">@drawable/button_next</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">15sp</item>

    </style>

    <style name="button_submit">
        <item name="android:background">@drawable/button_submit</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">15sp</item>

    </style>

    <style name="FreshCart_textView">
        <item name="android:background">@drawable/editext_background</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textColor">#868989</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>

    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
